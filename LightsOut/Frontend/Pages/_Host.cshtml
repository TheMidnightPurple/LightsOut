@page "/"
@namespace LightsOut.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>LightsOut</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="LightsOut.styles.css" rel="stylesheet" />
    <script type='text/javascript'
        src='https://www.bing.com/api/maps/mapcontrol?key=AvJBk8jswrwMBktJo7SQsVrPbDbz-q87DjiyLgIgj5eAScG46u6_P15z9saUT-et'></script>
    <script type='text/javascript' src="drawMapa.js"></script>
</head>

<body>
    <component type="typeof(App)" render-mode="Server" />
    
    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
    <script src="_framework/blazor.server.js"></script>
</body>

</html>
